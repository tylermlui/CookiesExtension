{
    "manifest_version": 3,
    "name": "Cookie extention",
    "description": "Will clear cookies on specified websites",
    "version": "1.0",
    "action": {
      "default_popup": "cookies.html",
      "default_icon": "cookie.png"
    },
      "icons": {
        "16": "images/cookie16.png",
        "32": "images/cookie32.png",
        "48": "images/cookie48.png",
        "128": "images/cookie128.png"
      }
  }